{
  "name": "MySecPol",
  "version": "6",
  "manifest_version": 2,
  
  "description": "MySecPol: Client-side policy based safe browsing",
  
  "permissions": ["http://*/*", "<all_urls>", "webRequest", "webRequestBlocking", "storage", "privacy","tabs","cookies"],
  
  "icons": {
    "48": "icons/test.png"
  },
  
"background": {
      "scripts": [ "variables.js","block.js" ,"pr_settings.js","ntab.js"]
   },

   "browser_action": {
    "default_icon": "icons/test.png",
    "default_popup": "popup.html"
  }

}
